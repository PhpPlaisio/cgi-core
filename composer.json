{
  "name": "plaisio/cgi-core",
  "description": "PhpPlaisio: CGI Core",
  "keywords": [
    "PhpPlaisio",
    "CGI",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "plaisio/cgi": "^1.0.0",
    "plaisio/exception": "^1.0.0",
    "plaisio/helper-html": "^1.0.0",
    "plaisio/helper-url": "^1.0.0",
    "plaisio/kernel": "^1.0.0",
    "setbased/helper-cast": "^2.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "plaisio/obfuscator-core": "^1.0.0",
    "plaisio/cgi-test": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Cgi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Cgi\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
