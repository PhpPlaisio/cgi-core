{
  "name": "plaisio/cgi-core",
  "description": "PhpPlaisio: CGI Core",
  "keywords": [
    "PhpPlaisio",
    "CGI",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/cgi": "^1.3.0",
    "plaisio/exception": "^1.2.0",
    "plaisio/helper-html": "^4.1.0",
    "plaisio/helper-url": "^1.2.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/obfuscator": "^2.1.0",
    "setbased/helper-cast": "^3.0.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/cgi-test": "^1.5.0",
    "plaisio/console-kernel": "^1.0.1",
    "plaisio/obfuscator-core": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Cgi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Cgi\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
